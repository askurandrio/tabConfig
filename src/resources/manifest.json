{
  "short_name": "TabConfig",
  "permissions": [
    "tabs",
    "alarms",
    "storage"
  ],
  "icons": {
    "19": "icon.png",
    "38": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "name": "TabConfig",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": ["spinner.svg"]
}
