{
  "short_name": "TabConfig",
  "permissions": [
    "tabs",
    "alarms"
  ],
  "name": "TabConfig",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.ico"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
